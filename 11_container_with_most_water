#11 container with most water
def max_area(height):
    l,r = 0,len(height)-1
    area = 0
    while l<r:
        area  = max(area,min(height[l],height[r]) * (r-l))
        if l<r:
            l+=1
        else:
            r-=1
    return area    
height = [1,8,6,2,5,4,8,3,7]
print(max_area(height))
